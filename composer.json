{
    "name": "mlab/webhook",
    "description": "Simple webhook server to manage api services",
    "repositories": {
        "JsonDatabase-storage": {
            "type": "github",
            "url": "https://github.com/marcodefelice/JsonDatabase-storage.git"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.9",
        "monolog/monolog": "^3.8",
        "symfony/http-foundation": "^7.2",
        "vlucas/phpdotenv": "^5.6",
        "php-di/php-di": "^7.0",
        "nikic/fast-route": "^1.3",
        "robmorgan/phinx": "^0.16.6",
        "doctrine/dbal": "^4.2",
        "illuminate/queue": "^12.0",
        "guzzlehttp/psr7": "^2.7",
        "illuminate/bus": "^12.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Mlab\\Webhook\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Marco De Felice",
            "email": "io@marcodefelice.it"
        }
    ],
    "minimum-stability": "alpha"
}
